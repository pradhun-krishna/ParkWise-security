# ANPR Parking System Configuration
# Copy this file to .env and fill in your actual values

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# Database Tables (Your existing table names)
PARKING_SESSIONS_TABLE=booking_sessions
PERMANENT_PARKING_TABLE=shobha_permanent_parking
PERMANENT_SESSIONS_TABLE=shobha_permanent_parking_sessions
PARKING_LOTS_TABLE=parking_lots

# Parking Configuration
PARKING_LOT_ID=1
HOURLY_RATE=50.0
MINIMUM_CHARGE=20.0

# Camera Configuration
# CCTV Cameras (RTSP - Recommended)
ENTRY_CAMERA_RTSP=rtsp://admin:password@192.168.1.100:554/stream1
EXIT_CAMERA_RTSP=rtsp://admin:password@192.168.1.101:554/stream1

# USB Cameras (Fallback)
ENTRY_CAMERA_USB=0
EXIT_CAMERA_USB=1

# GPIO Pin Configuration (Raspberry Pi)
BOOM_BARRIER_PIN=18
DISPLAY_PIN=19
LED_GREEN_PIN=20
LED_RED_PIN=21
BUZZER_PIN=22

# Payment Configuration
UPI_MERCHANT_ID=parkwise@upi
MERCHANT_NAME=ParkWise
PAYMENT_TIMEOUT=300

# System Configuration
DETECTION_COOLDOWN=5
BARRIER_OPEN_TIME=15

# Optional: External APIs
PLATE_RECOGNIZER_TOKEN=your_plate_recognizer_token_here
